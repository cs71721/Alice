{
  "name": "mental-health-intake-chatbot",
  "version": "1.0.0",
  "description": "Conversational mental health intake chatbot using Claude AI",
  "main": "server/index.js",
  "scripts": {
    "install-all": "npm install && cd client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "cd client && npm start",
    "start": "node server/index.js"
  },
  "keywords": ["mental-health", "chatbot", "claude", "intake"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
